{
  "name": "@netdata/netdata-ui",
  "version": "1.7.37",
  "description": "netdata UI kit",
  "main": "./lib/index.js",
  "files": [
    "/lib"
  ],
  "scripts": {
    "_drop_native": "./node_modules/rimraf/bin.js node_modules/@types/react-native/",
    "start": "yarn _drop_native && yarn testOut && yarn storybook-start",
    "storybook-start": "bash start.sh",
    "testOut": "yarn test --env=jsdom --json --outputFile=.jest-test-results.json",
    "build-storybook": "build-storybook -c .storybook -o public",
    "build": "yarn _drop_native && yarn testOut && build-storybook",
    "clean": "./node_modules/rimraf/bin.js ./lib",
    "ts-bundle": "yarn _drop_native && yarn clean && tsc && tsc -m es6 && node copy-assets && yarn resolveAlias",
    "test": "jest --config ./jest/config.js",
    "cp-cloud": "cp -R ./lib ../cloud-frontend/node_modules/@netdata/netdata-ui/",
    "to-cloud": "yarn ts-bundle && yarn cp-cloud",
    "cp-dashboard": "cp -R ./lib ../dashboard/node_modules/@netdata/netdata-ui/",
    "to-dashboard": "yarn ts-bundle && yarn cp-dashboard",
    "prepare": "install-peers && yarn ts-bundle",
    "resolveAlias": "tscpaths -p tsconfig.json -s ./src -o ./lib"
  },
  "keywords": [
    "uikit",
    "netdata-ui"
  ],
  "author": "netdata",
  "license": "ISC",
  "dependencies": {
    "@elastic/react-search-ui": "^1.5.1",
    "@elastic/search-ui-site-search-connector": "^1.5.1",
    "@netdata/react-filter-box": "^1.0.0",
    "@prismicio/client": "^5.1.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.21.1",
    "polished": "^4.0.3",
    "ramda": "^0.27.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-portal": "^4.2.0",
    "react-table": "^7.2.1",
    "react-use": "^15.3.4",
    "react-window": "^1.8.5"
  },
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^5.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:netdata/netdata-ui.git"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@netdata/eslint-config-netdata": "0.0.1",
    "@storybook/addon-actions": "^6.1.0",
    "@storybook/addon-centered": "^5.2.3",
    "@storybook/addon-jest": "^6.1.0",
    "@storybook/addon-knobs": "^6.1.0",
    "@storybook/addon-links": "^6.1.0",
    "@storybook/addon-storysource": "^6.1.0",
    "@storybook/addons": "^6.1.0",
    "@storybook/react": "^6.1.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.1",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/codemirror": "0.0.98",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.8",
    "@types/pegjs": "^0.10.2",
    "@types/ramda": "^0.27.32",
    "@types/react": "^16.9.5",
    "@types/react-portal": "^4.0.2",
    "@types/storybook__react": "^5.2.1",
    "@types/styled-components": "^5.1.2",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "cpx": "^1.5.0",
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "install-peers-cli": "^2.1.1",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "jsdom": "^16.4.0",
    "json-loader": "^0.5.7",
    "markdown-loader": "^6.0.0",
    "prettier": "^2.1.2",
    "raw-loader": "^4.0.2",
    "react-redux": "^7.1.1",
    "react-test-render": "^1.1.2",
    "react-test-renderer": "^17.0.1",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.3",
    "require-context": "^1.1.0",
    "rimraf": "^3.0.0",
    "storybook": "^6.1.0",
    "storybook-addon-styled-component-theme": "^1.2.5",
    "storybook-readme": "^5.0.8",
    "styled-components": "^5.2.1",
    "svg-sprite-loader": "^5.0.0",
    "svgo": "^1.3.0",
    "svgo-loader": "^2.2.1",
    "ts-jest": "^26.4.4",
    "tscpaths": "^0.0.9",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1"
  }
}
