{
  "name": "@netdata/netdata-ui",
  "version": "0.6.10",
  "description": "netdata UI kit",
  "main": "./lib/index.js",
  "files": [
    "/lib"
  ],
  "scripts": {
    "_drop_native": "./node_modules/rimraf/bin.js node_modules/@types/react-native/",
    "start": "npm run _drop_native && npm run testOut && npm run storybook-start",
    "storybook-start": "bash start.sh",
    "testOut": "jest --env=jsdom --json --outputFile=.jest-test-results.json",
    "build-storybook": "build-storybook -c .storybook -o public",
    "build": "npm run _drop_native && npm run testOut && build-storybook",
    "clean": "./node_modules/rimraf/bin.js ./lib",
    "ts-bundle": "npm run _drop_native && npm run clean && tsc && tsc -m es6 && node copy-assets",
    "test": "jest --env=jsdom",
    "prepare": "install-peers && npm run ts-bundle"
  },
  "keywords": [
    "uikit",
    "netdata-ui"
  ],
  "author": "netdata frontend team",
  "license": "ISC",
  "dependencies": {
    "@rmwc/button": "^5.7",
    "@rmwc/circular-progress": "^5.7",
    "polished": "^3.4.1",
    "ramda": "^0.26.1",
    "react-portal": "^4.2.0",
    "react-use": "^12.7.1"
  },
  "peerDependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "styled-components": "^4.4.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:netdata/netdata-ui.git"
  },
  "devDependencies": {
    "@netdata/eslint-config-netdata": "0.0.1",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-centered": "^5.2.3",
    "@storybook/addon-jest": "^5.2.3",
    "@storybook/addon-knobs": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addon-storysource": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "@testing-library/jest-dom": "^4.1.1",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.12",
    "@types/ramda": "^0.26.26",
    "@types/react": "^16.9.5",
    "@types/react-portal": "^4.0.2",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.19",
