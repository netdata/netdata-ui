{
  "name": "@netdata/netdata-ui",
  "version": "3.0.2",
  "description": "netdata UI kit",
  "main": "./lib/index.js",
  "files": [
    "/lib"
  ],
  "scripts": {
    "start": "yarn storybook dev",
    "storybook-start": "bash start.sh",
    "testOut": "yarn test --env=jsdom --json --outputFile=.jest-test-results.json",
    "build-storybook": "storybook build -c .storybook -o public",
    "build": "yarn testOut && storybook build",
    "clean": "rimraf ./lib",
    "copy-files": "copyfiles -u 1 src/**/*.d.ts src/**/*.svg lib/",
    "ts-bundle": "yarn clean && tsc && tsc -m es6 && yarn copy-files && yarn resolve-alias",
    "test": "jest --config ./jest/config.js",
    "cp-cloud": "cp -R ./lib ../cloud-frontend/node_modules/@netdata/netdata-ui/",
    "to-cloud": "yarn ts-bundle && yarn cp-cloud",
    "cp-dashboard": "cp -R ./lib ../dashboard/node_modules/@netdata/netdata-ui/",
    "to-dashboard": "yarn ts-bundle && yarn cp-dashboard",
    "cp-charts": "cp -R ./lib ../charts/node_modules/@netdata/netdata-ui/",
    "to-charts": "yarn ts-bundle && yarn cp-charts",
    "resolve-alias": "tsconfig-replace-paths -p tsconfig.json -s ./src -o ./lib",
    "prepublishOnly": "yarn clean && yarn ts-bundle"
  },
  "keywords": [
    "uikit",
    "ui-kit",
    "netdata-ui"
  ],
  "author": "netdata",
  "license": "ISC",
  "resolutions": {
    "@storybook/**/@emotion/sheet": "^1",
    "@storybook/**/@emotion/styled": "^11",
    "yargs": "^17.3"
  },
  "dependencies": {
    "@elastic/react-search-ui": "^1.20.2",
    "@elastic/search-ui-site-search-connector": "^1.20.2",
    "@prismicio/client": "7.3.0",
    "@styled-system/css": "^5.1.5",
    "@tanstack/react-table": "^8.10.6",
    "@tanstack/react-virtual": "3.0.0-beta.29",
    "axios": "^1.5.1",
    "polished": "^4.2.2",
    "react-beautiful-dnd": "^13.1.1",
    "react-portal": "^4.2.2",
    "react-select": "^5.7.7",
    "react-table": "^7.8.0",
    "react-use": "^17.4.0",
    "react-window": "^1.8.9",
    "styled-system": "^5.1.5",
    "throttle-debounce": "5.0.0",
    "use-context-selector": "^1.3.7"
  },
  "peerDependencies": {
    "lodash": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.9"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:netdata/netdata-ui.git"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@storybook/addon-actions": "^7.4.6",
    "@storybook/addon-centered": "^5.3.21",
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/addon-storysource": "^7.4.6",
    "@storybook/react": "^7.4.6",
    "@storybook/react-webpack5": "^7.4.6",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^20.8.4",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "copyfiles": "^2.4.1",
    "eslint": "^8.51.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "html-loader": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "install-peers-cli": "^2.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.1.1",
    "jsdom": "22.1.0",
    "lodash": "^4.17.21",
    "markdown-loader": "8.0.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "storybook": "^7.4.6",
    "storybook-readme": "^5.0.9",
    "styled-components": "^5.3.9",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^4.0.0",
    "ts-jest": "^29.1.1",
    "tsconfig-replace-paths": "^0.0.14",
    "typescript": "^4.9.5",
    "url-loader": "^4.1.1"
  }
}
