    "jest-styled-components": "^6.3.3",
    "jsdom": "^15.1.1",
    "json-loader": "^0.5.7",
    "markdown-loader": "^5.1.0",
    "prettier": "^1.18.2",
    "raw-loader": "^3.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-intl": "^3.3.2",
    "react-redux": "^7.1.1",
    "react-test-render": "^1.1.2",
    "redux": "^4.0.4",
